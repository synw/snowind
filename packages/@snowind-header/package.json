{
  "name": "@snowind/header",
  "version": "0.0.10",
  "description": "A responsive header with a mobile menu widget for Snowind",
  "repository": "https://github.com/synw/snowind",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "emit": "vue-tsc --declaration --emitDeclarationOnly",
    "serve": "vite preview",
    "localpub": "yarn publish --registry http://localhost:4873/"
  },
  "dependencies": {
    "@snowind/state": "^0.0.2",
    "install-peers": "^1.0.4",
    "vue": "^3.2.31"
  },
  "peerDependencies": {
    "vue": "^3.2.31"
  },
  "devDependencies": {
    "@babel/types": "^7.20.7",
    "@types/node": "^18.11.18",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/compiler-sfc": "^3.2.45",
    "tslib": "^2.4.1",
    "typescript": "^4.6.3",
    "vite": "^4.0.4",
    "vue-tsc": "^1.0.24"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "type": "module",
  "main": "./dist/header.umd.cjs",
  "module": "./dist/header.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/header.js",
      "require": "./dist/header.umd.cjs"
    }
  },
  "license": "MIT"
}