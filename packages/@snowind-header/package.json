{
  "name": "@snowind/header",
  "version": "0.0.11",
  "description": "A responsive header with a mobile menu widget for Snowind",
  "repository": "https://github.com/synw/snowind",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "emit": "vue-tsc --declaration --emitDeclarationOnly",
    "serve": "vite preview",
    "localpub": "yarn publish --registry http://localhost:4873/"
  },
  "dependencies": {
    "@snowind/state": "^0.0.2",
    "@vueuse/core": "^9.13.0",
    "vue": "^3.2.31"
  },
  "peerDependencies": {
    "vue": "^3.2.31"
  },
  "devDependencies": {
    "@babel/types": "^7.21.3",
    "@types/node": "^18.15.10",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue/compiler-sfc": "^3.2.47",
    "install-peers": "^1.0.4",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "vite": "^4.2.1",
    "vue-tsc": "^1.3.8"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "type": "module",
  "main": "./dist/header.umd.cjs",
  "module": "./dist/header.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/header.js",
      "require": "./dist/header.umd.cjs"
    }
  },
  "license": "MIT"
}