{
  "name": "@snowind/header",
  "version": "0.0.9",
  "description": "A responsive header with a mobile menu widget for Snowind",
  "repository": "https://github.com/synw/snowind",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "emit": "vue-tsc --declaration --emitDeclarationOnly",
    "serve": "vite preview",
    "localpub": "yarn publish --registry http://localhost:4873/"
  },
  "dependencies": {
    "@snowind/state": "^0.0.2",
    "install-peers": "^1.0.3"
  },
  "peerDependencies": {
    "vue": "^3.2.19"
  },
  "devDependencies": {
    "@babel/types": "^7.17.0",
    "@types/node": "^16.10.2",
    "@vitejs/plugin-vue": "^1.9.2",
    "@vue/compiler-sfc": "^3.2.19",
    "rollup-plugin-typescript2": "^0.30.0",
    "typescript": "^4.4.3",
    "vite": "^2.6.2",
    "vue-tsc": "^0.33.9"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "./dist/header.umd.js",
  "module": "./dist/header.es.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/header.es.js",
      "require": "./dist/header.umd.js"
    }
  },
  "license": "MIT"
}
