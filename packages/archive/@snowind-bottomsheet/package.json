{
  "name": "@snowind/bottomsheet",
  "version": "0.0.1",
  "description": "A mobile bottom sheet for Snowind",
  "repository": "https://github.com/synw/snowind",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview"
  },
  "peerDependencies": {
    "vue": "^3.2.31"
  },
  "dependencies": {
    "@vueuse/core": "^9.1.0"
  },
  "devDependencies": {
    "@iconify/json": "^2.1.26",
    "@types/node": "^16.10.2",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/compiler-sfc": "^3.2.31",
    "rollup-plugin-typescript2": "^0.31.2",
    "typescript": "^4.6.3",
    "vite": "^2.9.1",
    "vue-tsc": "^0.40.1"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/bottomsheet.umd.js",
  "module": "./dist/bottomsheet.es.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/bottomsheet.es.js",
      "require": "./dist/bottomsheet.umd.js"
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT"
}
