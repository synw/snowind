{
  "name": "@snowind/stepper",
  "version": "0.1.0",
  "description": "A progress stepper widget for Snowind",
  "repository": "https://github.com/synw/snowind",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview"
  },
  "peerDependencies": {
    "vue": "^3.2.19"
  },
  "devDependencies": {
    "@types/node": "^20.5.9",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vue/compiler-sfc": "^3.2.19",
    "rollup-plugin-typescript2": "^0.35.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vue-tsc": "^1.8.10"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/stepper.umd.js",
  "module": "./dist/stepper.mjs",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/stepper.mjs",
      "require": "./dist/stepper.umd.js"
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT"
}